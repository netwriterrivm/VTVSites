<?xml version="1.0" encoding="utf-8" ?>
<!--COLUMNCONTAINER -->
<edx:template  xmlns:edx="msnbc-edx-edit-view" name="bannercont" type="container" uiname="rsi.edx.column">
	<edx:match element="link_object_class">
		<span edxtemplate="region_link_object_class_image" edxpath="."></span>
	</edx:match>
	<edx:match element="link_object_document">
		<span edxtemplate="region_link_object_document_image" edxpath="."></span>
	</edx:match>
	<edx:match element="link_object_map">
		<span edxtemplate="region_link_object_map_image" edxpath="."></span>
	</edx:match>
	<edx:match element="link_object_url">
		<span edxtemplate="region_link_object_url_image" edxpath="."></span>
	</edx:match>
	<edx:match element="object_binary">
		<span edxtemplate="object_binary_image_writer" edxpath="."></span>
	</edx:match>
	<!-- EMPTY - note "magic" name for the situation when the container is empty -->
	<edx:match element="#empty">
		<div edxtemplate="emptycell" edxpath="."></div>
	</edx:match>
</edx:template>