<?xml version="1.0" encoding="utf-8" ?>
<!-- LIST CONTAINER -->
<edx:template xmlns:edx="msnbc-edx-edit-view" name="listcontainer" type="container" uiname="rsi.menu.insert.list">
	<edx:match element="listitem">
		<li edxtemplate="listitem" edxpath="." edxoptions="enter-action:checkshift;allow-split:true"></li>
	</edx:match>
	<edx:match element="list">
		<span edxtemplate="list" edxpath="." edxoptions="display-link:layout"></span>
	</edx:match>
	<edx:match element="#empty">
		<div edxtemplate="emptycell" edxpath="."></div>
	</edx:match>
</edx:template>