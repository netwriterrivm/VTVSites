<?xml version="1.0" encoding="utf-8" ?>
<!--COLUMNCONTAINER -->
<edx:template  xmlns:edx="msnbc-edx-edit-view" name="columncont" type="container" uiname="rsi.edx.column">
	<edx:match element="topicallead">
		<div edxtemplate="topicallead" edxpath="."></div>
	</edx:match>
	<edx:match element="header" edxpath=".">
		<div edxtemplate="header" edxpath="." edxoptions="display-link:level;enter-action:checkshift;allow-split:true"></div>
	</edx:match>
	<edx:match element="paragraph">
		<div edxtemplate="paragraph" edxpath="." edxoptions="display-link:layout;enter-action:checkshift;allow-split:true"></div>
	</edx:match>
	<edx:match element="link_object_binary_image">
		<div edxtemplate="region_link_object_binary_image" edxoptions="display-link:align" edxpath="."></div>
	</edx:match>
	<edx:match element="datatable">
		<div edxtemplate="datatable" edxpath="."></div>
	</edx:match>
	<edx:match element="list">
		<div edxtemplate="list" edxpath="." edxoptions="display-link:layout"></div>
	</edx:match>
	<edx:match element="definitionlist">
		<div edxtemplate="definitionlist" edxpath="." edxoptions="display-link:layout"></div>
	</edx:match>
	<edx:match element="caption">
		<div edxtemplate="caption" edxpath="." edxoptions="enter-action:checkshift;allow-split:true"></div>
	</edx:match>
	<edx:match element="link_object_map_swf">
		<div edxtemplate="region_link_object_map_swf" edxpath="."></div>
	</edx:match>
	<edx:match element="link_content">
		<div edxtemplate="region_link_content" edxpath="."></div>
	</edx:match>
	<edx:match element="link_object_wuc">
		<div edxtemplate="region_link_object_wuc" edxpath="."></div>
	</edx:match>
	<edx:match element="link_object_external">
		<div edxtemplate="region_link_object_external" edxpath="."></div>
	</edx:match>
	<edx:match element="subgroup">
		<div edxtemplate="subgroup" edxpath="."></div>
	</edx:match>
	<edx:match element="dotnetcontrol">
		<div edxtemplate="dotnetcontrol" edxpath="."></div>
	</edx:match>
	<edx:match element="banner">
		<div edxtemplate="banner" edxpath="."></div>
	</edx:match>
	<edx:match element="box">
		<div edxtemplate="box" edxpath="."></div>
	</edx:match>
	<edx:match element="media_player">
		<div edxtemplate="media_player" edxpath="." edxoptions="display-link:type"></div>
	</edx:match>
	<!-- EMPTY - note "magic" name for the situation when the container is empty -->
	<edx:match element="#empty">
		<div edxtemplate="emptycell" edxpath="."></div>
	</edx:match>
</edx:template>