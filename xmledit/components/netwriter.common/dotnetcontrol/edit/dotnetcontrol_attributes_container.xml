<?xml version="1.0" encoding="utf-8" ?>
<edx:template xmlns:edx="msnbc-edx-edit-view" name="dotnetcontrol_attributes_container" type="container"
	uiname="rsi.edx.dotnetcontrol.attributes">
	<img edxtemplate="widget:icon.default" />
	<edx:match element="#text">
		<span edxtemplate="field:flow" edxpath="."></span>
	</edx:match>
	<edx:match element="dotnetcontrol_attribute">
		<span edxtemplate="dotnetcontrol_attribute" edxpath="."></span>
	</edx:match>
	<!-- EMPTY - note "magic" name for the situation when the container is empty -->
	<edx:match element="#empty">
		<div edxtemplate="emptycell" edxpath="."></div>
	</edx:match>
</edx:template>
