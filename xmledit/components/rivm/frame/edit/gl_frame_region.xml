	<edx:insert>
		<frame layout="Normaal 1 Koloms">
			<framemenu layout="hidden"></framemenu>
			<column1>
				<header align="" level="2">[Plaats hier een kopregel]</header>
			</column1>
			<column2></column2>
			<column3></column3>
			<column4></column4>
			<column5></column5>
			<framelabel visibilityinframe="hidden">[label]</framelabel>
			<frameid>id</frameid>
		</frame>
	</edx:insert>
	<edx:navigationitem enabled="false"  toolbar_button_visible="0"  shortcut_ctrl="false" shortcut_alt="false" shortcut_key ="" shortcut_keycode="" name="rsi.menu.insert.frame" icon="/NetWriterGui/content/xml/icons/insert_frame_below2.gif" id="btnInsertFrame" onclick="" xml_tag="frame"  enable_check="guiOwnerCanApplyTag(\'frame\');" aspx_propertypage=""  aspx_selectpage="" item_type="insert"></edx:navigationitem>
	<edx:attributelist>
		<attribute name="layout" uiname="rsi.edx.attribute.frame.layout" applytocolumn="false" uiicon="">
			<option value="Normaal 1 Koloms">rsi.edx.attribute.frame.normal.1</option>
			<option value="Normaal 2 Koloms">rsi.edx.attribute.frame.normal.2</option>
			<option value="Kader 1 Koloms">rsi.edx.attribute.frame.boxed.1</option>
			<option value="Kader 2 Koloms">rsi.edx.attribute.frame.boxed.2</option>
			<option value="icon: Normaal 1 Koloms">rsi.edx.attribute.frame.normal.1.top</option>
			<option value="icon: Normaal 2 Koloms">rsi.edx.attribute.frame.normal.2.top</option>
			<option value="icon: Kader 1 Koloms">rsi.edx.attribute.frame.boxed.1.top</option>
			<option value="icon: Kader 2 Koloms">rsi.edx.attribute.frame.boxed.2.top</option>
			<option value="Klap in Klap uit">rsi.edx.attribute.frame.klapinklapuit</option>
			<option value="Dynamische Content">rsi.edx.attribute.frame.dynamic</option>
		</attribute>
		<childattribute childname="framemenu" name="layout" uiname="rsi.edx.attribute.frame.layout.framemenu"
			uiicon="">
			<option value="hidden">rsi.edx.attribute.no</option>
			<option value="hidden_line">rsi.edx.attribute.framemenu.no.seperator.yes</option>
			<option value="visible">rsi.edx.attribute.yes</option>
			<option value="visible_noline">rsi.edx.attribute.framemenu.yes.seperator.no</option>
			<option value="visible_newline">rsi.edx.attribute.framemenu.yes.seperator.no.lf</option>
			<option value="visible_newline_noline">rsi.edx.attribute.framemenu.yes.seperator.yes.lf</option>
		</childattribute>
		<childattribute childname="framelabel" name="visibilityinframe" uiname="rsi.edx.attribute.frame.layout.framelabel"
			uiicon="">
			<option value="hidden">rsi.edx.attribute.no</option>
			<option value="visible">rsi.edx.attribute.yes</option>
		</childattribute>
	</edx:attributelist>
</edx:template>
